{
    "data_loader": {
        "train_data_augmentation_settings": {
            "rotation_range": 20,
            "width_shift_range": 0.2,
            "height_shift_range": 0.2,
            "zoom_range": 0.2,
            "shear_range": 0.1
        },
        "test_data_augmentation_settings": {},
        "target_size": 224,
        "train_size": 0.8,
        "batch_size": 32,
        "random_state": 42
    },
    "model": {
        "number_of_categories": 102,
        "dropout_rate": 0.2,
        "learning_rate": 1e-3,
        "learning_rate_shrinker": 100,
        "metrics": "accuracy",
        "loss": "categorical_crossentropy"
    },
    "trainer": {
        "early_stopping_patience": 3,
        "number_of_base_epochs": 1,
        "number_of_fine_tune_epochs": 1,
        "validation_split": 0.8,
        "verbose_training": true,
        "percentage_of_frozen_layers": 0.8
    }
}
